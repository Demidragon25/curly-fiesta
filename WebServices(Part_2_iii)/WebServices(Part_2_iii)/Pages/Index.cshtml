@page
@model WebServices_Part_2_iii.Pages.IndexModel


<!DOCTYPE html>
<html>
<head>
    <title>Session Values</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet">
</head>
<body>
    <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
        <div class="container">
            <a class="navbar-brand" href="#">CurlyFiesta</a>
        </div>
    </nav>

    <div class="container mt-4">
        <div class="card">
            <div class="card-header">
                Session Dictionary Values
            </div>
            <div class="card-body">
                <table class="table">
                    <thead>
                        <tr>
                            <th>Key</th>
                            <th>Value</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td>Time</td>
                            <td>@HttpContext.Session.GetString("Time")</td>
                        </tr>
                        <tr>
                            <td>User Agent</td>
                            <td>@HttpContext.Session.GetString("UserAgent")</td>
                        </tr>
                        <tr>
                            <td>Remote IP</td>
                            <td>@HttpContext.Session.GetString("RemoteIP")</td>
                        </tr>
                        <tr>
                            <td>Project Name</td>
                            <td>@HttpContext.Session.GetString("ProjectName")</td>
                        </tr>
                        <tr>
                            <td>Lambda Test</td>
                            <td>@HttpContext.Session.GetString("LambdaTest")</td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>
</body>
</html>
